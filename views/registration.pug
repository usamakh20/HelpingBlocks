extends layout2

block content 
    nav.navbar.navbar-default.navbar-fixed
      .container-fluid
        .navbar-header
          button.navbar-toggle(type='button' data-toggle='collapse' data-target='#navigation-example-2')
            span.sr-only Toggle navigation
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.navbar-brand(href='#') Registration
        .collapse.navbar-collapse
          ul.nav.navbar-nav.navbar-left
            li.dropdown
              a.dropdown-toggle(href='#' data-toggle='dropdown')
                i.fa.fa-globe
                b.caret.hidden-sm.hidden-xs
                span.notification.hidden-sm.hidden-xs 5
                p.hidden-lg.hidden-md
                  | 2 Notifications
                  b.caret
              ul.dropdown-menu
                li
                  a(href='#') Notification 1
                li
                  a(href='#') Notification 2
          ul.nav.navbar-nav.navbar-right
            li.dropdown
              a.dropdown-toggle(href='#' data-toggle='dropdown')
                p
                  | Options
                  b.caret
              ul.dropdown-menu
                li
                  a(href='#') Option
                li
                  a(href='#') Option 2
                li
                  a(href='#') Option 3
                li
                  a(href='#') Option 3
                li.divider
                li
                  a(href='#') Separated Option
            li
              a(href='#')
                p Log out
            li.separator.hidden-lg.hidden-md
    .col-md-8
      .card
        .header
          h4.title Register Recipients
        .content
          form
            .row
              .col-md-4
                .form-group
                  label Name
                  input.form-control(type='text' placeholder='Name' value='')
              .col-md-4
                .form-group
                  label Contact No.
                  input.form-control(type='number' placeholder='Contact No.')
              .col-md-4
                .form-group
                  label CNIC
                  input.form-control(type='number'  maxlength="13" size="13")                  
            .row
              .col-md-12
                .form-group
                  label Address
                  input.form-control(type='text' placeholder='Shop Address' value='')
              .col-md-4
                .form-group
                  label City
                  input.form-control(type='text' placeholder='City' value='')
              .col-md-3
                .form-group
                  label Gender
                  select.form-control
                    option(value='Male') Male
                    option(value='Female') Female
                    option(value='Other') Other
              .col-md-2
                .form-group
                  label Age
                  input.form-control(type='number' placeholder='Age' value='')
            button.btn.btn-info.btn-fill.pull-right(type='submit') Register Recipient
            .clearfix        
    .col-md-8
      .card
        .header
          h4.title Register Shopkeeper
        .content
          form
            .row
              .col-md-4
                .form-group
                  label Name
                  input.form-control(type='text' placeholder='Name' value='')
              .col-md-4
                .form-group
                  label Contact No.
                  input.form-control(type='number' placeholder='Contact No.')
              .col-md-4
                .form-group
                  label CNIC
                  input.form-control(type='number'  maxlength="13" size="13")                  
            .row
              .col-md-12
                .form-group
                  label Shop Address
                  input.form-control(type='text' placeholder='Shop Address' value='')
              .col-md-4
                .form-group
                  label City
                  input.form-control(type='text' placeholder='City' value='')  
            button.btn.btn-info.btn-fill.pull-right(type='submit') Register Shopkeeper
            .clearfix
